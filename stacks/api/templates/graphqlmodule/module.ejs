import * as typeDefs from './schema.graphql'
import resolvers from './resolvers'
import Provider from './provider'

const provider = new Provider('<%= collection %>')

export default {
    autoload: true,
    typeDefs,
    resolvers,
    context: (ctx) => { 
        const context = { ...ctx }
        context['<%= name.capitalize() %>Provider'] = provider

        return context
    },
    imports: []
}
