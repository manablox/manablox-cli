{
  "name": "manablox-cli",
  "version": "1.2.3",
  "description": "zero config webpack runner",
  "main": "bin/manablox",
  "repository": "https://github.com/manablox/manablox-cli.git",
  "author": "daspete <daspetemail@gmail.com>",
  "license": "MIT",
  "bin": {
    "manablox": "./bin/manablox"
  },
  "scripts": {
    "release": "standard-version; git push --follow-tags origin master; npm publish"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "webpack",
    "backpack",
    "es",
    "ecmascript",
    "manablox",
    "modules"
  ],
  "dependencies": {
    "@babel/core": "^7.7.7",
    "babel-loader": "^8.0.6",
    "babel-preset-backpack": "^0.8.4",
    "commander": "^4.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "inquirer": "^7.0.1",
    "json-loader": "^0.5.7",
    "nodemon": "^2.0.2",
    "ramda": "^0.26.1",
    "source-map-support": "^0.5.16",
    "webpack": "^4.41.4",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "commitizen": "^4.0.3",
    "husky": "^3.1.0",
    "standard-version": "^7.0.1"
  }
}
